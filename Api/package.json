{
  "name": "Api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:compose": "docker-compose up -d",
    "dev": "nodemon src/server.ts",
    "db:init": "docker build -t mysql-img -f ./prisma/DockerFile . && docker run -p 3306:3306 -d --rm --name statec-db mysql-img",
    "db:migration": "docker exec -i statec-db mysql -uroot -ptestStatec < prisma/init.sql && npx prisma migrate up --experimental",
    "test": "jest --watch"
  },
  "dependencies": {
    "@prisma/client": "^2.11.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "onesignal-node": "^3.2.0",
    "socket.io": "^3.0.1"
  },
  "devDependencies": {
    "@prisma/cli": "^2.11.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.7",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
